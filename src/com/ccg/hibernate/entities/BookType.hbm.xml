<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping.dtd">
<hibernate-mapping package="com.ccg.hibernate.entities">
	<class name="BookType" table="SH_BOOK_TYPE">
		<id name="typeId" column="TYPE_ID" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="typeName" column="TYPE_NAME" type="java.lang.String"></property>
	</class>
</hibernate-mapping>